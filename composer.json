{
    "name": "psychemedics/twygo-api",
    "description": "Pacote para comunicação com a API do sistema Twygo",
    "keywords": ["laravel", "Twygo", "API"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergio Hess",
            "email": "sergio.hess@psychemedics.com.br"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6",
        "illuminate/support": "5.1.x|5.2.x|5.3.x|5.4.x|5.5.x|5.6.x|5.7.x"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "PsyTwygo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PsyTwygo\\ServiceProvider"
            ]
        }
    }
}